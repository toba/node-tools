{
   "name": "@toba/node-tools",
   "description": "Toba Node Tools",
   "version": "1.4.18",
   "private": false,
   "publishConfig": {
      "access": "public"
   },
   "main": "cjs/index.js",
   "module": "esm/index.js",
   "types": "esm/index.d.ts",
   "files": [
      "esm",
      "cjs"
   ],
   "author": {
      "name": "Jason Abbott"
   },
   "license": "MIT",
   "scripts": {
      "build:esm": "./node_modules/.bin/tsc --project ./node_modules/@toba/develop/tsconfig.esm.json",
      "build:cjs": "./node_modules/.bin/tsc --project ./node_modules/@toba/develop/tsconfig.cjs-min.json",
      "prepublishOnly": "npm run build:esm & npm run build:cjs",
      "test": "jest --coverage"
   },
   "devDependencies": {
      "@toba/develop": "^6.7.13",
      "@toba/test": "^4.1.17"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/toba/node-tools"
   },
   "dependencies": {
      "@toba/tools": "^6.13.1",
      "@types/node": "^13.1.1"
   }
}
