{
   "name": "@toba/utility",
   "description": "Toba Utility",
   "version": "1.6.0",
   "private": false,
   "publishConfig": {
      "access": "public"
   },
   "main": "",
   "files": [
      "index.ts",
      "lib/constants.ts",
      "lib/is.ts",
      "lib/number.ts",
      "lib/regex.ts",
      "lib/storage.ts",
      "lib/text.ts",
      "lib/time.ts",
      "lib/url.ts",
      "lib/utility.ts"
   ],
   "author": {
      "name": "Jason Abbott"
   },
   "license": "MIT",
   "scripts": {
