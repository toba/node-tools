{
   "name": "@toba/utility",
   "description": "Toba Utility",
   "version": "1.0.0-beta.1",
   "private": false,
   "main": "./src/utility.ts",
   "author": {
      "name": "Jason Abbott"
   },
   "license": "MIT",
   "scripts": {
      "test": "node_modules/.bin/mocha --recursive"
   },
   "jest": {
      "transform": {
         "^.+\\.tsx?$": "ts-jest"
      },
      "transformIgnorePatterns": [
         "<rootDir>/node_modules/(?!@toba)"
      ],
      "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
      "moduleFileExtensions": [
         "ts",
         "tsx",
         "js",
         "jsx",
         "json",
         "node"
      ]
   },
   "devDependencies": {
      "@toba/test": "^1.0.0-beta.5",
      "@toba/tslint": "^1.0.1",
      "@types/chai": "^4.0.1",
      "@types/jest": "^22.0.0",
      "jest-codemods": "^0.13.8",
      "prettier": "^1.9.2",
      "ts-jest": "^22.0.0",
      "tslint": "^5.5.0",
      "typescript": "^2.6.2"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/toba/utility"
   },
   "bugs": {
      "url": "https://github.com/toba/utility/issues"
   },
   "dependencies": {}
}
