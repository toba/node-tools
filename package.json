{
   "name": "@toba/utility",
   "description": "Toba Utility",
   "version": "1.0.8",
   "private": false,
   "publishConfig": {
      "access": "public"
   },
   "main": "",
   "files": ["index.ts", "lib/storage.ts", "lib/utility.ts"],
   "author": {
      "name": "Jason Abbott"
   },
   "license": "MIT",
   "scripts": {
      "test": "jest"
   },
   "jest": {
      "setupFiles": ["jest-localstorage-mock"],
      "transform": {
         "^.+\\.tsx?$": "ts-jest"
      },
      "transformIgnorePatterns": ["<rootDir>/node_modules/(?!@toba)"],
      "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
      "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
   },
   "devDependencies": {
      "@toba/lint": "^1.0.8",
      "@toba/test": "^1.0.0",
      "prettier": "^1.9.2",
      "typescript": "^2.6.2"
   },
   "prettier": {
      "arrowParens": "avoid",
      "bracketSpacing": true,
      "printWidth": 80,
      "semi": true,
      "singleQuote": false,
      "tabWidth": 3,
      "trailingComma": "none",
      "useTabs": false
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/toba/utility"
   },
   "bugs": {
      "url": "https://github.com/toba/utility/issues"
   },
   "dependencies": {}
}
